for i = 1: POSCAR_name_struct_num
    cd(PATH_CEF);
    POSCAR_name = POSCAR_name_struct(i).name;
    filename = POSCAR_name + ".cif";
    mkdir(POSCAR_name);   
    cd(POSCAR_name);
    [Rm,sites,Atom_name,Atom_num,~]=POSCAR_readin(strcat(POSCAR_dir,'/',POSCAR_name));
    sites = Standard_sites(sites);
    [SG,R_struct] = cif_gen(filename,Rm,Atom_name,sites,1,'CEF_cif');
    Rf_list = [[sites.rc1]',[sites.rc2]',[sites.rc3]'];
    Rf_list_new = Rf_list/Ns;
    Rm_new = Ns * Rm;
    for ion_name = ion_list 
        if strcontain(POSCAR_name,ion_name)
            break;
        end
    end
    CEF_python(POSCAR_name, ion_name,python_path);
    fprintf("######## %d #######\n",i);
    fprintf("%s %s SG: %d\n",POSCAR_name,ion_name,SG);
    fprintf("a:%7.4f c:%7.4f\n",norm(Rm_new(1,:)),norm(Rm_new(3,:)));
    fprintf(" A: %7.4f %7.4f %7.4f\n",Rf_list_new(1,1),Rf_list_new(1,2),Rf_list_new(1,3));
    fprintf(" R: %7.4f %7.4f %7.4f\n",Rf_list_new(2,1),Rf_list_new(2,2),Rf_list_new(2,3));
    fprintf("X1: %7.4f %7.4f %7.4f\n",Rf_list_new(3,1),Rf_list_new(3,2),Rf_list_new(3,3));
    fprintf("X2: %7.4f %7.4f %7.4f\n",Rf_list_new(4,1),Rf_list_new(4,2),Rf_list_new(4,3));
end
